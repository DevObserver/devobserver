{
  "name": "@devobserver/web-extension",
  "version": "1.5.1",
  "description": "",
  "author": "Pochtar Eduard <iamfrntdv@gmail.com>",
  "scripts": {
    "dev": "vite",
    "dev:browser": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "graphql-codegen": "graphql-codegen --config ./codegen.yml",
    "graphql-codegen-public": "graphql-codegen --config codegen/codegen-api-public.yml"
  },
  "dependencies": {
    "@apollo/client": "3.7.13",
    "axios": "1.4.0",
    "class-variance-authority": "0.6.0",
    "graphql": "16.6.0",
    "javascript-time-ago": "2.5.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroller": "1.2.6",
    "react-router-dom": "6.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@types/react": "18.2.1",
    "@types/react-dom": "18.2.2",
    "@types/react-infinite-scroller": "1.2.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@vitejs/plugin-react": "4.0.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vite": "4.3.4",
    "vite-plugin-svgr": "3.2.0"
  }
}
