{
  "name": "@devobserver/web-extension",
  "version": "1.2.0",
  "description": "",
  "author": "Pochtar Eduard <iamfrntdv@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:browser": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "graphql-codegen": "graphql-codegen --config ./codegen.yml",
    "graphql-codegen-public": "graphql-codegen --config codegen/codegen-api-public.yml"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@vanilla-extract/css": "1.9.2",
    "@vanilla-extract/dynamic": "2.0.3",
    "@vanilla-extract/recipes": "0.3.0",
    "@vanilla-extract/sprinkles": "1.5.1",
    "@vanilla-extract/vite-plugin": "3.7.0",
    "axios": "1.2.0",
    "graphql": "16.6.0",
    "javascript-time-ago": "2.5.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroller": "1.2.6",
    "react-router-dom": "6.4.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.14.1",
    "@graphql-codegen/typescript": "2.8.2",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-infinite-scroller": "1.2.3",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@vitejs/plugin-react": "2.2.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "prettier": "2.8.0",
    "typescript": "4.9.3",
    "vite": "3.2.4",
    "vite-plugin-svgr": "2.2.2"
  }
}
